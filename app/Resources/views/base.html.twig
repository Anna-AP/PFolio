<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Anna-Ang&eacute;lique PORTE - D&eacute;veloppeuse Web{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
            {% stylesheets  
                '@AppPortfolioBundle/Resources/public/css/style.css'
                '@AppAdminBundle/Resources/public/css/style.css'
            %}
                <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
            {% endstylesheets %}
            <script src="{{ asset('js/jquery.min.js') }}"></script>
            <script src="{{ asset('js/jquery-ui.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.js') }}"></script>
            <script src="https://use.fontawesome.com/ee4c1a4e12.js"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container content_nav">
                <button class="navbar-toggle" data-toggle="collapse" data-target=".navHeaderCollapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <div class="collapse navbar-collapse navHeaderCollapse">
                    <ul class="nav navbar-nav navbar-left col_lg-offset-0">
                        <li><a href="{{ path('app_portfolio_homepage') }}">Home</a></li>
                        <li><a href="{{ path('app_actualite_homepage') }}">News</a></li>
                        <li><a href="{{ path('app_portfolio_realisations') }}">Projects</a></li>
                        <li><a href="{{ path('app_support_homepage') }}">Support</a></li>
                        <li><a href="{{ path('app_portfolio_about_me') }}">About me</a></li>
                        <li><a href="{{ path('app_forum_homepage') }}">Forum</a></li>
                        <li><a href="{{ path('app_portfolio_contact') }}">Contact</a></li>
                    </ul>
                </div>
            </div>
        </div>

        {% block body %}{% endblock %}
        {% block javascript %}{% endblock %}
    </body>
</html>
